.page_Wrapper.width100
  .header
    .top_header
      .head_title
        %h2
          Practice
          = succeed "est" do
            %span.tile_t T
      .head_timer
        %h4#clock
          %i.fa.fa-clock-o{"aria-hidden" => "true"}
          %span.time
            Time Remaining
            %span 14:21
        %h4#all_ques
          %i.fa.fa-book{"aria-hidden" => "true"}
          %span.number 1 of 44
    .btm_header.padd
      %a.calc{href: "javascript:",'data-toggle'=>"modal", 'data-target'=>"#calc-modal"}
        %i.fa.fa-calculator{"aria-hidden" => "true"}
        = succeed "alculator" do
          %span C
      %a.calc{href: "#"}
        %i.fa.fa-flag-checkered{"aria-hidden" => "true"}
        = succeed "lag" do
          %span F
  .para_contant.ipad_scroll.mob_scroll.contant_scroll
    %p There are eight apartments on a floor; each apartment has either one, two, three or four rooms, as shown in the figure. Each apartment is owned by one of four owners: Adam, Flora, Gary or Rose. Each owner owns two apartments.
    %p.mrgbtm0
      %span.d_block Both of Adam's apartments are in the same horizontal row but do not share a wall.
      = succeed "Gary's" do
        %span.d_block Either Rose or Flora have the same number of rooms in their two apartments.
    / %img.mrgbtm20{src: "images/que_img_2.jpg"}/
    = image_tag 'https://gradready.s3.ap-southeast-2.amazonaws.com/static/ucat/que_img_2.jpg'
    %p The 1-room apartments belong to which of the following owners?
    %ul.radio_ans_list
      %li
        .radio_wrap
          %input#a.answer{name: "answer", type: "radio"}/
          %label#a_text{for: "a"} Gary and Flora
      %li
        .radio_wrap
          %input#b.answer{name: "answer", type: "radio"}/
          %label#b_text{for: "b"} Flora and Adam
      %li
        .radio_wrap
          %input#c.answer{name: "answer", type: "radio"}/
          %label#c_text{for: "c"} Gary and Adam
      %li
        .radio_wrap
          %input#d.answer{name: "answer", type: "radio"}/
          %label#d_text{for: "d"} Both belong to Rose
  .footer
    .end_exam
      %a.ftr_btn.btn_bdr_rgt{"data-target" => "#endexam_modal", "data-toggle" => "modal", href: "#"}
        %i.fa.fa-step-forward{"aria-hidden" => "true"}
        = succeed "nd" do
          %span e
    .end_exam.step_btn_wrap
      %a.ftr_btn.btn_bdr_lft{href: "/ucat_exercises/question2"}
        %i.fa.fa-arrow-left{"aria-hidden" => "true"}
        = succeed "revious" do
          %span p
      %a.ftr_btn.btn_bdr_lft{href: "/ucat_exercises/question4"}
        %span> n
        ext
        %i.fa.fa-arrow-right{"aria-hidden" => "true"}
#endexam_modal.modal.fade{"aria-hidden" => "true", "aria-labelledby" => "exampleModalLabel", role: "dialog", tabindex: "-1"}
  .modal-dialog.exam_ready_modal.end_review_modal{role: "document"}
    .modal-content
      .modal-header
        %h5#exampleModalLabel.modal-title End Exam
        %button.close{"aria-label" => "Close", "data-dismiss" => "modal", type: "button"}
          / %img{src: "images/close.png"}/
          = image_tag 'https://gradready.s3.ap-southeast-2.amazonaws.com/static/ucat/close.png'
      .modal-body
        %p.mrgbtm20
          / %img{src: "images/que_not.png"}>/
          = image_tag 'https://gradready.s3.ap-southeast-2.amazonaws.com/static/ucat/que_not.png'
          You have chosen to end this exam
        %p Are you sure you want to end this exam
      .modal-footer
        %button.btn.btn-secondary{"data-dismiss" => "modal", onclick: "window.location.href='/ucat/dashboard/home'", type: "button"} yes
        %button.btn.btn-secondary{"data-dismiss" => "modal", type: "button"} no


:javascript
  $('#all_ques').click(function(){
   $('.number').toggleClass('none');
   });

  $('#clock').click(function(){
   $('.time').toggleClass('none');
  });
