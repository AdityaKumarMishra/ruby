.panel.panel-default
  .panel-heading
    %h3.panel-title Sessions
  .panel-body
    %table.table.table-bordered.table-hover
      %thead
        %tr
          %th Start Time
          %th End time
          %th Class name
          %th Frequency
      %tbody
        -course_version.sessions.each do |session|
          %tr
            %td= session.start_time.to_formatted_s(:long)
            %td= session.end_time.to_formatted_s(:long)
            %td= session.student_class.name
            %td= session.frequency


