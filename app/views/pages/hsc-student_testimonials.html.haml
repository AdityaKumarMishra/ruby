  var nav_type = "#{params[:type]}"

  if(nav_type == 'interviewready'){
    initialize_owl($('#owl-demo1'));
  }else{
    initialize_owl($('#owl-demo'));
  }

  $('a[href="#grad_testi"]').on('shown.bs.tab', function () {
    initialize_owl($('#owl-demo'));
  });

  $('a[href="#interview_testi"]').on('shown.bs.tab', function () {
    initialize_owl($('#owl-demo1'));
  });

  function initialize_owl(el) {
    el.owlCarousel({
      margin: 3,
      loop: true,
      autoplay: true,
      autoPlayTimeout: 1000,
      nav: true,
      dots: true,
      slideSpeed : 100,
      responsive: {
        0: {
          items: 1,
          nav: true
        },
        600: {
          items: 1,
          nav: false
        },
        1000: {
          items: 3,
          nav: true,
          loop: false
        }
      }
    });
  }
