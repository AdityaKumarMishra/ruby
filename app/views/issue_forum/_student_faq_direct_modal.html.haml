:css
  .mrgn_left{
    margin-left: 20px;
  }
  .fnt_size{
    font-size: 16px;
    color: #5c5c5c;
  }
  .btn_wrap_mdl{
    display: flex;
    justify-content: center;
    align-items: baseline;
  }
  .btn_wrap_mdl button{
    margin-right: 10px;
  }
  .btn_wrap_mdl .red_btn{
    color: #fff;
    background-image: linear-gradient(to bottom, #f80101 0%, #f80101 100%) !important;
  }
  .btn_wrap_mdl .red_btn:before{
    background-image: linear-gradient(to bottom, #f80101 0%, #f80101 100%) !important;
  }
  #faqStdDirect .modal-header{
    padding: 15px 15px 0 15px;
    border-bottom: 0;
  }
  #faqStdDirect .modal-body{
    text-align: center;
  }
  #faqStdDirect .modal-header h2{
    font-size: 24px;
    margin-top: 20px;
  }
  #faqStdDirect .modal-header .close {
    margin-top: -29px;
    margin-right: -29px;
  }
  #faqStdDirect .mtop_20{
    margin-top: 20px;
  }
  #faqStdDirect .modal-dialog{
    position: relative;
    top: 23%;
  }
  .h2_clss:before {
    content: "";
    position: absolute;
    width: 48px;
    height: 10px;
    top: 70%;
    margin-top: 0;
    background-image: url(https://gradready.s3.ap-southeast-2.amazonaws.com/static/public_pages/points.svg);
    background-repeat: no-repeat;
    left: 0px;
  }

  .h2_clss:after {
    content: "";
    position: absolute;
    width: 48px;
    height: 10px;
    top: 70%;
    margin-top: 0;
    background-image: url(https://gradready.s3.ap-southeast-2.amazonaws.com/static/public_pages/points.svg);
    background-repeat: no-repeat;
    right: 0px;
  }

  .h2_clss{
    position: relative;
    margin-bottom: 20px;
    width: auto;
  }

  .h2_clss h2 {
    margin: 0;
