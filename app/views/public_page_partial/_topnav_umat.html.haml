              %ul.contact_list
                %li
                  %p
                    Questions?
                  %ul
                    %li
                      %a{href: "/umat-preparation-courses/faq"} FAQ
                    %li
                      %a{href: "/blog/umat-preparation-courses"} BLOG
                    %li
                      %a{href: "/umat-preparation-courses/contact"} GET IN TOUCH
                    %li
                      %a{href: "/jobs/available_positions"} JOIN US 
                %li
                  %p
                    About
                  %ul
                    %li
                      %a{href: "/umat-preparation-courses/about"} OVERVIEW
                    %li
                      %a{href: "/umat-preparation-courses/team"} TEAM
                    %li
                      %a{:href => "/updates?tab=UMAT"} RECENT UPDATES

            - if user_signed_in?
              / %li
              /   %a{href: '/dashboard/home'} Dashboard
              %li.contact
                %a{href: "javascript:void(0)"}
                  .fa.fa-cart-plus
                - if current_user.student?
                  %ul.contact_list
                    %li
                      %p CART
                      %ul
                        %li
                          = link_to 'CART', user_ongoing_order_link if current_user.student?
                        %li
                          = link_to "PAST ORDERS", orders_path if current_user.student? && current_user.orders.where.not(status: 0).present?
              %li.contact
                %a{href: "javascript:void(0)"} Account
                %ul.contact_list
                  %li
                    %p ACCOUNT
                    %ul
                      %li
                        %a{href: '/dashboard/home'} DASHBOARD
                      %li
                        = link_to 'PROFILE', edit_user_registration_path
                      %li
                        = link_to "LOG OUT", destroy_user_session_path, :method => :delete
            - else
              %li
                %a{href: "/login"} Login

:javascript

  $(".recommender_popup").click(function(){
    $("#course-recommender .cr").addClass("cr--on")
  });
