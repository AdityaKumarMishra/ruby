              %a{href: "https://www.facebook.com/GradReadyUMAT"}
                = image_tag 'https://gradready.s3.ap-southeast-2.amazonaws.com/static/public_pages/fb.svg', alt: 'img'
      .col-sm-12
        .tp.top_right
          %ul
            %li.contact
              %a{href: "javascript:void(0)"} Courses
              %ul.contact_list
                %li
