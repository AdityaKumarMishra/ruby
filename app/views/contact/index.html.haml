:javascript
  jQuery(function(t){t(".mis-stage").miSlider({speed: 700, stageHeight: 300, slidesContinuous: !0, slidesOnStage: !1, slidePosition: "center", slideStart: "mid", slideScaling: 160, offsetV: -5, centerV: !0, navButtonsShow: !0, navButtons: !1, navButtonsOpacity: 1}); t(".richText").richText()})

  AOS.init({
    easing: "ease-in-out-sine"
  });

  $("#email-back-btn").click(function(){
    $('#faqDirect').modal('show');
  });

  $("#faq-yes-btn").click(function(){
    window.location.href = "/gamsat-preparation-courses/faq";
  });

  $("#faq-no-btn").click(function(){
    $(".qa_query_form").addClass("show_qa_query_form");
    $(".qa_query_form").find('.qa_form_header h3').html("Got a Question?");
  });

