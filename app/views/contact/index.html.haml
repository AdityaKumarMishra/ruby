            - if @product_line == 'umat'
              = image_tag 'https://gradready.s3.ap-southeast-2.amazonaws.com/static/public_pages/umat_call_us_icon.svg', alt: 'Call Us'
            - else
              = image_tag 'https://gradready.s3.ap-southeast-2.amazonaws.com/static/public_pages/call_us_icon.svg', alt: 'Call Us'
