              / %span or
              %a#call_back_btn.hvr-shutter-in-horizontal.default_btn.submit_tkt{href: "javascript:void(0)"} Submit Ticket
          #email-back-btn.aos-item.call_contact_box.hvr-shutter-out-horizontal{"data-aos" => "fade-left", "data-aos-duration" => "1100"}
            - if @product_line == 'umat'
              = image_tag 'https://gradready.s3.ap-southeast-2.amazonaws.com/static/public_pages/umat_email_us_icon.svg', alt: 'Emails'
            - else
              = image_tag 'https://gradready.s3.ap-southeast-2.amazonaws.com/static/public_pages/email_us_icon.svg', alt: 'Emails'
